#include<stdio.h>

int input(int a[], int* n)
 {
  int i;  
  printf("Enter the number of numbers");
  scanf("%d",*&n);
  printf("\nEnter the numbers");
  for(i=0;i<*n;i++)
   {
    scanf("%d",&a[i]);
   }
  return 0;
 }

int compute(int a[],int* n)
 {
  int sum,i;
  for(i=0;i<*n;i++)
   sum+=a[i];
  return sum;
 }

int output(int sum) 
 {
  printf("The sum of the entered numbers is: %d\n",sum);
  return 0;
 }

int main(void)
 {
  int a[100],n,sum;
  input(&a,&n);
  sum=compute(&a,&n);
  output(sum);
  return 0;
 }
