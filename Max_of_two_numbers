#include<stdio.h>

int input(int* a,int *b)
 {
  printf("Enter the two numbers:\n");
  scanf("%d\n%d\n",*&a,*&b);
  return 0;
 }

int compute(int* a,int* b)
 {
  if(*a>*b)
   return *a;
  else if(a==b)
   return 0;
  else 
   return b;
 }
 
int output(int max)
 {
  if(max==0)
    printf("The numbers entered are equal!");
  else
    printf("The maximum number of the two numbers is:%d\n");
    return 0;
 }

int main(void)
 {
  int a,b,max;
  input(&a,&b);
  max=compute(&a,&b);
  output(max);
  return 0;
 }  
